package scene

import br.ufes.inf.lprm.situation.*;
import br.ufes.inf.lprm.scene.base.*;
import scene.fever.*
import br.ufes.inf.lprm.scene.util.SituationHelper;

rule "FeverSituation"
@role(situation)
@type(fever.Fever)
    when
    	f1: Person(temperature > 37)
    then
    	SituationHelper.situationDetected(drools);
end